{
	"name": "ParazunAmazon",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"description": "API to obtain summary of any amazon product",
		"parameters": {
			"asins": {
				"type": "string",
				"defaultValue": "B07FXYLSMB%2CB000PKMAOY%2CB08DS45Y8W"
			},
			"region": {
				"type": "string",
				"defaultValue": "US"
			},
			"page": {
				"type": "int",
				"defaultValue": 1
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "https://parazun-amazon-data.p.rapidapi.com/product/summaries/",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous",
			"authHeaders": {
				"X-RapidAPI-Host": {
					"type": "SecureString",
					"value": "**********"
				},
				"X-RapidAPI-Key": {
					"type": "SecureString",
					"value": "**********"
				}
			}
		}
	}
}