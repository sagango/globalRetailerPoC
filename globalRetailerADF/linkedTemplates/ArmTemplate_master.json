{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "globalRetailerADF"
		},
		"MockarooCustomer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MockarooCustomer'"
		},
		"fileStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fileStorage'"
		},
		"globalRetailerDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'globalRetailerDB'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=sagaweb.database.windows.net;Initial Catalog=globalRetailerDB"
		},
		"MockarooCustomer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.mockaroo.com/api/3fe85af0?count=1000&key=c02c14f0"
		},
		"MockarooCustomer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "jose@sagaweb.org"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "globalRetailerADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MockarooCustomer_password": {
						"value": "[parameters('MockarooCustomer_password')]"
					},
					"fileStorage_connectionString": {
						"value": "[parameters('fileStorage_connectionString')]"
					},
					"globalRetailerDB_connectionString": {
						"value": "[parameters('globalRetailerDB_connectionString')]"
					},
					"MockarooCustomer_properties_typeProperties_url": {
						"value": "[parameters('MockarooCustomer_properties_typeProperties_url')]"
					},
					"MockarooCustomer_properties_typeProperties_userName": {
						"value": "[parameters('MockarooCustomer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}